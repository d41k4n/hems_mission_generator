	<webosm_road_filter>
		<filter>(way[highway=primary]({{bbox}});way[highway=secondary]({{bbox}});way[highway=tertiary]({{bbox}});way[highway=residential]({{bbox}});way[highway=service]({{bbox}});way[highway=living_street]({{bbox}}););out;</filter>
	</webosm_road_filter>

	<webosm_parking_filter>
		<filter>way["parking"="surface"][!"building"](if:is_closed()%26%26length()>100)({{bbox}});out+center;</filter>
	</webosm_parking_filter>

	<webosm_pitches_filter>
		<filter>way["leisure"="pitch"][!"building"](if:is_closed()%26%26length()>100)({{bbox}});way._["sport"!="tennis"]({{bbox}});way._["sport"!~"shooting.*"]({{bbox}});out+center;</filter>
	</webosm_pitches_filter>

